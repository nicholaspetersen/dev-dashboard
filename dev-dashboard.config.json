{
  "version": "1.0",
  "scanPaths": ["~/Sites"],
  "projects": [
    {
      "id": "aibl",
      "name": "AIBL",
      "path": "~/Sites/aibl",
      "type": "nextjs",
      "processes": [
        {
          "name": "dev",
          "command": "npm run dev",
          "port": 3002,
          "portArg": "--port"
        }
      ]
    },
    {
      "id": "transcriber",
      "name": "Podcast Transcriber",
      "path": "~/Sites/transcriber",
      "type": "nextjs",
      "processes": [
        {
          "name": "dev",
          "command": "npm run dev",
          "port": 3003,
          "portArg": "--port"
        }
      ]
    },
    {
      "id": "trade-journal",
      "name": "Trade Journal",
      "path": "~/Sites/trade-journal",
      "type": "monorepo",
      "processes": [
        {
          "name": "server",
          "command": "npm run dev:server",
          "port": 3001
        },
        {
          "name": "client",
          "command": "npm run dev:client",
          "port": 5173,
          "dependsOn": ["server"]
        }
      ]
    },
    {
      "id": "create-nav-prototype",
      "name": "Create Navigation Prototype",
      "path": "~/Sites/Create navigation prototype",
      "type": "vite",
      "processes": [
        {
          "name": "dev",
          "command": "npm run dev",
          "port": 5174,
          "portArg": "--port"
        }
      ]
    }
  ],
  "portRanges": {
    "nextjs": [3000, 3010],
    "vite": [5173, 5183],
    "express": [3001, 3020]
  }
}
